@model Skill
@{
    ViewData["Title"] = "AddNewSkill";
    Layout = "~/Views/Layout/Index.cshtml";
}

<div class="content">
    <div class="container-fluid">
        <h4 class="page-title">Skill</h4>
        <div class="row">
            <div class="col-md-12">
                <div class="card">
                    <div class="card-header">
                        <div class="card-title">Add Your New Skill Here !</div>
                    </div>
                    <div class="card-body">

                        <form method="post">
                            
                            <label>Title</label>
                            <input type="text" asp-for="Title" class="form-control" />
                            <br />
                            <label>Date Range</label>
                            <input type="number" asp-for="Value" class="form-control" />
                            <br />                          
                            <button class="btn btn-primary">Add Skill</button>


                        </form>


                    </div>
                </div>


            </div>

        </div>
    </div>
</div>

